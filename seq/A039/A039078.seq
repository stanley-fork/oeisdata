%N A039078 Numbers whose base-9 representation has the same number of 0's and 2's.
%t A039078 Select[Range[120],DigitCount[#,9,0]==DigitCount[#,9,2]&] (* _Harvey P. Dale_, Mar 17 2021 *)
%o A039078 (MATLAB)
%o A039078 a = NaN(10000,1); k = 0; j = 1;
%o A039078 while any(isnan(a))
%o A039078     d = dec2base(k,9)-'0';
%o A039078     if sum(d==0)==sum(d==2)
%o A039078         a(j) = k; j = j+1;
%o A039078     end
%o A039078     k = k+1;
%o A039078 end
%o A039078 disp(regexprep(num2str(a'),' +',', ')) % _Chris R. Rehmann_, Feb 23 2026
